{
  "manifest_version": 2,

  "name": "ArabicRemover",
  "description": "Remove the killer arabic text for webkit CoreText on iOS/OS X",
  "version": "0.1",
  "author": "Rafael Fidelis <rafa_fidelis@yahoo.com.br>",
  "permissions": [],
  "icons": { 
    "16": "icons/icon_16.jpg",
    "46": "icons/icon_46.jpg",
    "96": "icons/icon_96.jpg",
    "112": "icons/icon_112.jpg",
    "128": "icons/icon_128.jpg" 
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*" , "<all_urls>"],
      "js": ["script.js"]
    }
  ]
}